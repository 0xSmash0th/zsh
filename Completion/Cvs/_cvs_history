#autoload

# "+Tacelow?D:b:f:m:n:p:r:t:u:x:X:z:"
_arguments -s \
  -{T,a,c,e,l,o,w,\?} \
  '-D+:date:_cvs_D' \
  '-b+:string:' \
  '-f+:arg:' \
  '-m+:module:_cvs_modules' \
  '-n+:arg:' \
  '*-p+:repository:' \
  '-r+:rev:' \
  '-t+:tag:' \
  '-u+:user name:' \
  '-x+:type:_cvs_history_x' \
  '-X+:arg:' \
  '-z+:arg:' \
  '*:file:_cvs_files'
