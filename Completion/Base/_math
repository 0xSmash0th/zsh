#compdef -math-

local expl

if [[ "$PREFIX" = *[^a-zA-Z0-9_]* ]]; then
  IPREFIX="$IPREFIX${PREFIX%%[a-zA-Z0-9_]#}"
  PREFIX="${PREFIX##*[^a-zA-Z0-9_]}"
fi
if [[ "$SUFFIX" = *[^a-zA-Z0-9_]* ]]; then
  ISUFFIX="${SUFFIX##[a-zA-Z0-9_]#}$ISUFFIX"
  SUFFIX="${SUFFIX%%[^a-zA-Z0-9_]*}"
fi

_description expl parameter
compgen "$expl[@]" -v
