#compdef pnmgamma

local ret=1

if [[ CURRENT -eq 2 ]]; then
  _message 'gamma value or red gamma value'
elif [[ CURRENT -eq 3 ]]; then
  _pbm_file && ret=0
  _message 'green gamma value'
elif [[ CURRENT -eq 4 ]]; then
  _message 'blue gamma value'
elif [[ CURRENT -eq 5 ]]; then
  _pbm_file && ret=0
fi

return ret
