#compdef sched

local expl lines

if [[ CURRENT -eq 2 ]]; then
  if compset -P -; then
    lines=(${(f)"$(sched)"})
    _description expl 'scheduled jobs'
    [[ -z $lines ]] || compadd "$expl[@]" -ld lines - {1..$#lines}
  else
    _message 'time specification'
    return 1
  fi
fi
compset -n 3 && _normal
